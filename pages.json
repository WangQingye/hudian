{
	"easycom": {
		"^u-(.*)": "@/uview-ui/components/u-$1/u-$1.vue"
	},
	"pages": [{
		"path": "pages/index/index",
		"style": {
			"navigationBarTitleText": "uni-ui基础项目"
		}
	}, {
		"path": "pages/task-desc/task-desc",
		"style": {
			"navigationBarTitleText": "任务详情"
		}

	}, {
		"path": "pages/my-info/my-info",
		"style": {
			"navigationBarTitleText": "我的",
			"enablePullDownRefresh": false
		}

	}, {
		"path": "pages/task-list/task-list",
		"style": {
			"navigationBarTitleText": "",
			"enablePullDownRefresh": false
		}

	}, {
		"path": "pages/finish-task/finish-task",
		"style": {
			"navigationBarTitleText": "完成详情",
			"enablePullDownRefresh": false
		}

	}],
	"globalStyle": {
		"navigationBarTextStyle": "white",
		"navigationBarTitleText": "uni-app",
		"navigationBarBackgroundColor": "#007AFF",
		"backgroundColor": "#FFFFFF"
	},
	"tabBar": {
		"color": "#7A7E83",
		"selectedColor": "#3cc51f",
		"borderStyle": "black",
		"backgroundColor": "#ffffff",
		"height": "50px",
		"fontSize": "10px",
		"iconWidth": "24px",
		"spacing": "3px",
		"list": [{
			"pagePath": "pages/index/index",
			"iconPath": "static/jiarufangjian-01.png",
			"selectedIconPath": "static/jiarufangjian-01.png",
			"text": "广场"
		}, {
			"pagePath": "pages/my-info/my-info",
			"iconPath": "static/haoyou.png",
			"selectedIconPath": "static/haoyou.png",
			"text": "我的"
		}]
	}
}
